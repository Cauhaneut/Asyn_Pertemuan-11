<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.rohit_pert11.asyn">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:parentActivityName="com.rohit_pert11.asyn.MainActivity"
                  android:name="com.rohit_pert11.asyn.MainActivity"

                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.rohit_pert11.asyn.loading"
            android:parentActivityName="com.rohit_pert11.asyn.MainActivity">
        </activity>
        <activity
                android:name="com.rohit_pert11.asyn.RepositoryActivity"
                android:parentActivityName="com.rohit_pert11.asyn.MainActivity"
                />

    </application>

</manifest>
